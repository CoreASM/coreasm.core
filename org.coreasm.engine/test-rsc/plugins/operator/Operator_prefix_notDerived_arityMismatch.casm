CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

function test: LIST * LIST -> ELEMENT

operator prefix 50 '!!' = map
operator prefix 50 '?'  = map
operator prefix 50 '##' = test
operator prefix 50 '~'  = test

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + (?[0]))
  next
    print ("02: " + (!![0]))
  next
    print ("03: " + (~[1]))
  next
    print ("04: " + (##[1]))
