CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

function test: LIST -> ELEMENT

operator index 50 !! E! = head
operator index 50 ?  E? = head
operator index 50 ## E# = test
operator index 50 ~  E~ = test

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + ([0]?[0]E?))
  next
    print ("02: " + ([0]!![0]E!))
  next
    print ("03: " + ([0]~[1]E~))
  next
    print ("04: " + ([0]##[1]E#))
