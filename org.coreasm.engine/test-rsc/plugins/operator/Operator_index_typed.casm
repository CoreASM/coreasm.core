CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

operator index 50 !! E! on STRING * STRING = err
operator index 50 ?  E? on STRING * STRING = err
operator index 50 ## E# on STRING * STRING = err
operator index 50 ~  E~ on STRING * STRING = err
operator index 50 !! E! on NUMBER * NUMBER = dec
operator index 50 ?  E? on NUMBER * NUMBER = dec
operator index 50 ## E# on NUMBER * NUMBER = inc
operator index 50 ~  E~ on NUMBER * NUMBER = inc

derived dec(x, y) = y - 1
derived inc(x, y) = y + 1
derived err(x, y) = "fail"

