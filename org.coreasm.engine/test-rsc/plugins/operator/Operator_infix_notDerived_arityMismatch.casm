CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

function test: LIST * LIST * LIST -> ELEMENT

operator infixl 50 !! = head
operator infixl 50 ?  = head
operator infixl 50 ## = test
operator infixl 50 ~  = test

derived id(x) = x

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + ([0]?(@id)))
  next
    print ("02: " + ([0]!!(@id)))
  next
    print ("03: " + ([0]~[1]))
  next
    print ("04: " + ([0]##[1]))
