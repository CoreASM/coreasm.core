CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

function test: LIST * LIST -> ELEMENT

operator postfix 50 !! = map
operator postfix 50 ?  = map
operator postfix 50 ## = test
operator postfix 50 ~  = test

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + ([0]?))
  next
    print ("02: " + ([0]!!))
  next
    print ("03: " + ([1]~))
  next
    print ("04: " + ([1]##))
