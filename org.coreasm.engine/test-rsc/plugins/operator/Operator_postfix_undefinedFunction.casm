CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

operator postfix 50 '!!' = xyz
operator postfix 50 '?' = xyz
operator postfix 50 '##' = xyz
operator postfix 50 '~' = xyz

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + ([0]?))
  next
    print ("02: " + ([0]!!))
  next
    print ("03: " + ([1]~))
  next
    print ("04: " + ([1]##))
