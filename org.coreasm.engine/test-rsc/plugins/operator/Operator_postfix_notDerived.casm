CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

function test: LIST -> ELEMENT

operator postfix 50 '!!' = head
operator postfix 50 '?' = head
operator postfix 50 '##' = test
operator postfix 50 '~' = test

/*
 * @minsteps 1
 * @maxsteps 1
 *
 * @require "01: 0"
 * @require "02: 0"
 * @require "03: undef"
 * @require "04: undef"
 */
rule initRule =
  seq
    print ("01: " + ([0]?))
  next
    print ("02: " + ([0]!!))
  next
    print ("03: " + ([1]~))
  next
    print ("04: " + ([1]##))
