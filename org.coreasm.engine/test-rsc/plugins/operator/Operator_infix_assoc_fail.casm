CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

operator infixl 50 '!' = fun1
operator infixr 50 '?'  = fun2
operator infixn 50 '#' = fun3

derived fun1(x, y) = x - y
derived fun2(x, y) = x / y
derived fun3(x, y) = x * y

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + (1#2#3#4))
  next
    print ("02: " + (1!2!3!4))
  next
    print ("03: " + (1?2?3?4))
  next
    print ("04: " + (1?2!3#4))
  next
    print ("05: " + (1?2?3!4))
  next
    print ("06: " + (1?2!3!4))
  next
    print ("07: " + (1?2#3#4))
  next
    print ("08: " + (1!2!3#4))
  next
    print ("00: " + (1!2?3#4))
  next
    print ("10: " + (1#2!3#4))
