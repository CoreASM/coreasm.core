CoreASM Test

use StandardPlugins
use OperatorPlugin

init initRule

operator index 50 !! E! = xyz
operator index 50 ?  E? = xyz
operator index 50 ## E# = xyz
operator index 50 ~  E~ = xyz

/*
 * @minsteps 1
 * @maxsteps 1
 */
rule initRule =
  seq
    print ("01: " + ([0]?(@id)E?))
  next
    print ("02: " + ([0]!!(@id)E!))
  next
    print ("03: " + ([0]~[1]E~))
  next
    print ("04: " + ([0]##[1]E#))
